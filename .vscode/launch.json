{
    // Use IntelliSense to find out which attributes exist for C# debugging
    // Use hover for the description of the existing attributes
    // For further information visit https://github.com/OmniSharp/omnisharp-vscode/blob/master/debugger-launchjson.md
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Music.Netease Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build:Music.Netease",
            // If you have changed target frameworks, make sure to update the program path.
            "program": "${workspaceFolder}/Music.Netease/bin/Debug/netcoreapp3.1/Music.Netease.dll",
            "args": [],
            "cwd": "${workspaceFolder}/Music.Netease",
            // For more information about the 'console' field, see https://aka.ms/VSCode-CS-LaunchJson-Console
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".Fangzi.Bot Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build:Fangzi.Bot",
            // If you have changed target frameworks, make sure to update the program path.
            "program": "${workspaceFolder}/Fangzi.Bot/bin/Debug/net6.0/Fangzi.Bot.dll",
            "args": [],
            "cwd": "${workspaceFolder}",
            // For more information about the 'console' field, see https://aka.ms/VSCode-CS-LaunchJson-Console
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}